@model IEnumerable<StratasFair.BusinessEntity.Front.SubAdminModel>



    @{
        if (Model != null)
        {
            if (Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td data-title="First Name" class="f-name">
                            <a href="javascript:void(0)" onclick="ShowUserPrivilege(@item.UserClientId)" class="text-orange" data-toggle="modal" data-target="#view-user-detal">@item.FirstName</a>
                        </td>
                        <td data-title="Last Name" class="l-name">@item.LastName</td>
                        <td data-title="Email Address" class="email-id">
                            @item.Email
                        </td>
                        <td data-title="Position" class="position">@item.Position</td>
                        <td data-title="Added On" class="added-on">@item.CreatedOn</td>
                        <td data-title="Action" class="action">
                            <a href="javascript:void(0)" onclick="EditUserDetails(@item.UserClientId)" data-toggle="modal" data-target="#edit-sub-admin">
                                <i class="fa fa-pencil" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Edit"></i>
                            </a>

                            <a href="javascript:void(0);" onclick="DeleteUserDetails(@item.UserClientId);" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Delete">
                                <i class="fa fa-trash" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Delete"></i>
                            </a>

                            <a href="javascript:void(0);" onclick="ResendCredentialsMail(@item.UserClientId)" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Resent Credentials Mail">
                                <i class="fa fa-reply"></i>
                            </a>
                            <div class="switch-box small-switch">
                                <div class="switch">
                                    @{
                    if (@item.Status == 1)
                    {
                        <input id="cmn-toggle-@item.UserClientId" class="cmn-toggle cmn-toggle-round" type="checkbox" checked onchange="ActivateDeactivateClick(this, @item.UserClientId)">
                                        <label for="cmn-toggle-@item.UserClientId"></label>
                    }
                    else
                    {
                        <input id="cmn-toggle-@item.UserClientId" class="cmn-toggle cmn-toggle-round" type="checkbox" onchange="ActivateDeactivateClick(this, @item.UserClientId)">
                                        <label for="cmn-toggle-@item.UserClientId"></label>
                    }
                                    }

                                </div>
                            </div>
                        </td>
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td style="text-align:center"  data-title="First Name" colspan="9" class="f-name">
                        <span class="text-orange">No Record Found</span>
                    </td>
                </tr>
            }

        }
}

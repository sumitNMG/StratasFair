@model StratasFair.BusinessEntity.Front.GeneralInformationModel

<div class="modal-body">
    <h5 class="text-center">View General Information</h5>
    <form action="" class="form-section">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="form-group">
                    <label>Title</label>
                    @Html.TextBoxFor(m => m.Title, new { @class = "form-control", @readonly = true })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="form-group">
                    <label>Description</label>
                    @Html.TextAreaFor(m => m.Description, new { @class = "form-control", @readonly = true })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="form-group">
                    <label>Upload File</label>
                    @Html.TextAreaFor(m => m.ActualUploadFile, new { @class = "form-control", @readonly = true })
                </div>
            </div>
        </div>
       </form><!-- Form End -->
</div>
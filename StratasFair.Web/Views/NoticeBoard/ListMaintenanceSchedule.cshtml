@model IEnumerable<StratasFair.BusinessEntity.Front.MaintenanceScheduleModel>

@{
    if (Model != null)
    {
        if (Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td data-title="Common Area" class="subject">@item.CommonAreaName</td>
                    <td data-title="Description" class="message">@item.Description</td>
                    <td data-title="Meeting Time" class="meeting">@item.ScheduleDate</td>
                    <td data-title="Added On" class="added-on"> @item.CreatedOn</td>
                    <td data-title="Action" class="action">
                        <a href="javascript:void(0)" onclick="EditMaintenanceScheduleDetails(@item.MaintenanceScheduleId)" data-toggle="modal" data-target="#edit-schedule" data-placement="bottom" title="" data-original-title="Edit">
                            <i class="fa fa-pencil"></i>
                        </a>
                        <a href="javascript:void(0)" onclick="DeleteMaintenanceScheduleDetails(@item.MaintenanceScheduleId)" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Delete">
                            <i class="fa fa-trash"></i>
                        </a>
                    </td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td style="text-align:center" data-title="First Name" colspan="9" class="f-name">
                    <span class="text-orange">No Record Found</span>
                </td>
            </tr>
        }
    }
}

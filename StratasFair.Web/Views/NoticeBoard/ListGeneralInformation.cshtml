@model IEnumerable<StratasFair.BusinessEntity.Front.GeneralInformationModel>

@{
    if (Model != null)
    {
        if (Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td data-title="Title" class="subject">@item.Title</td>
                    <td data-title="Description" class="message">@item.Description</td>
                    <td data-title="File" class="file-name">
                        <a href="javascript:void(0)"><i class="fa fa-file"></i> @item.ActualUploadFile</a>
                    </td>
                    <td data-title="Added On" class="added-on">@item.CreatedOn</td>
                    <td data-title="Action" class="action">
                        <a href="javascript:void(0)" onclick="EditGeneralInformationDetails(@item.GeneralInformationId)" data-toggle="modal" data-target="#edit-information" data-placement="bottom" title="" data-original-title="Edit">
                            <i class="fa fa-pencil"></i>
                        </a>
                        <a href="javascript:void(0)" onclick="DeleteGeneralInformationDetails(@item.GeneralInformationId)" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Delete">
                            <i class="fa fa-trash"></i>
                        </a>
                        <a href="javascript:void(0)" onclick="ViewGeneralInformationDetails(@item.GeneralInformationId)" data-toggle="modal" data-target="#view-information" data-placement="bottom" title="" data-original-title="View">
                            <i class="fa fa-eye"></i>
                        </a>
                    </td>
                </tr>

            }
        }
        else
        {
            <tr>
                <td style="text-align:center" data-title="First Name" colspan="9" class="f-name">
                    <span class="text-orange">No Record Found</span>
                </td>
            </tr>
        }
    }
}
